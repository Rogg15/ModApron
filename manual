        repeat task.wait()until game:GetService("Workspace")["_wave_num"].Value >= 7

        local args = {
            [1] = "{27360a6e-9377-490d-9ef4-d0c6fe58a755}",
            [2] = CFrame.new(-28.61199188232422, 2.600001335144043, -158.9420166015625, 1, 0, -0, -0, 1, -0, 0, 0, 1)
        }
        
