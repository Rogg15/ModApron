        repeat task.wait()until game:GetService("Workspace")["_wave_num"].Value >= 8
        ussop()
        repeat task.wait()until game:GetService("Workspace")["_wave_num"].Value >= 9
        ussop()
        repeat task.wait()until game:GetService("Workspace")["_wave_num"].Value >= 11
        ussop()
        repeat task.wait()until game:GetService("Workspace")["_wave_num"].Value >= 12
        ussop()
